@media only screen and (min-width: 968px) {
  %%SELECTOR%% {
    {% if design.media.alignment == 'media-left' %}
     .wlt-content-container {
      	padding:0;
       	.wlt-content-action {
          padding:66px 33px 66px 0;
        }
      }
      &.with-video {
       .wlt-content-container {
          padding:0;
          .wlt-media {
            .video-wrapper {
              .pattern {
                left:calc(-93%/5);
              }
            }
          }
          &.round-top {
            .wlt-media {
              .video-wrapper {
                .pattern {
                  border-radius:0 99px 0 0;
                }
              }
            }
          }
          &.round-bottom {
            .wlt-media {
              .video-wrapper {
                .pattern {
                  border-radius:0 0 99px 0;
                }
              }
            }
          }
          &.round-both {
            .wlt-media {
              .video-wrapper {
                .pattern {
                  border-radius:0 99px 99px 0;
                }
              }
            }
          }
        }
      }
    {% elseif design.media.alignment == 'media-right' %}
     .wlt-content-container {
       padding:0;
       .wlt-content-action {
          padding:66px 0 66px 33px;
        }
      }
      &.with-video {
        padding:0;
        .wlt-media {
          .video-wrapper {
            .pattern {
              right:calc(-93%/5);
            }
          }
        }
        &.round-top {
          .wlt-media {
            .video-wrapper {
              .pattern {
                border-radius:0 99px 0 0;
              }
            }
          }
        }
        &.round-bottom {
          .wlt-media {
            .video-wrapper {
              .pattern {
                border-radius:0 0 99px 0;
              }
            }
          }
        }
        &.round-both {
          .wlt-media {
            .video-wrapper {
              .pattern {
                border-radius:0 99px 99px 0;
              }
            }
          }
        }
      }
    {% endif %}
  }
}
@media only screen and (min-width: 1120px) {
  %%SELECTOR%% {
    {% if design.media.alignment == 'media-left' %}
      padding:0;
    .wlt-content-container {
      .wlt-content-action {
        padding:0;
      }
    }
    {% elseif design.media.alignment == 'media-right' %}
     padding:0;
    .wlt-content-container {
      .wlt-content-action {
        padding:0;
      }
    }
    {% endif %}
  }
}