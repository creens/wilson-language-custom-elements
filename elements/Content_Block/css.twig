    /* Aligntment settings */
    {% if design.media.alignment == 'media-left' %}
      .wlt-content-container {
        padding:66px 0 0;
      }
      .wlt-content-action {
        order:1;
      }
      .wlt-media {
        order:0;
        margin-left:auto;
        &:after {
          display:none;
        }
        .img-contain {
          .pattern {
            left:calc(-93%/2);
          }
        }
      }
    {% elseif design.media.alignment == 'media-right' %}
   	 .wlt-content-container {
        padding:0 0 66px;
      }
      .wlt-content-action {
        order:0;
      }
      .wlt-media {
        order:1;
        margin-right:auto;
        &:before {
          display:none;
        }
        .pattern {
          transform:scaleX(-1);
        }
        .img-contain {
          .pattern {
            right:calc(-93%/2);
          }
        }
      }
    {% endif %}
