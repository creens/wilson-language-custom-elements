/* Spacing */
%%SELECTOR%% {
  margin-top: {{ design.spacing.margin_top.style }};
  margin-bottom: {{ design.spacing.margin_bottom.style }};
}
/* Image settings */
%%SELECTOR%% {
  .wlt-content-container {
    .wlt-media {
      .img-contain {
        .breakdance-image-clip {
          .breakdance-image-object {
            {% if design.media.zoom %}
              transform: scale({{ (design.media.zoom.number / 100) + 1 }}) translateZ(0);
            {% endif %}

            {% if design.media.focus_point %}
              transform-origin: {{ design.media.focus_point.x }}% {{ design.media.focus_point.y }}%;
            {% endif %}
          }
        }
      }
    }
  }
}
/* Theme color settings */
%%SELECTOR%% {
{% if content.theme_color.color == 'blue' %}
    .wlt-content-copy {
      .eyebrow {
        border-left:3px #003DA5 solid;
      }
    }
    .wlt-media {
      {% if content.theme_color.adjust_color == 'lighter' %}
        &:before, &:after {
          background: #CBD8F0;
        }
    {% elseif content.theme_color.adjust_color == 'darker' %}
        &:before, &:after {
          background: #002B79;
        }
    {% else %}
      &:before, &:after {
        background:#003DA5;
      }
    {% endif %}
      .breakdance-image-clip {
        border:9px #003DA5 solid;
      }
    }
    .wlt-btns {
      .wlt-btn-2 {
        color:#003DA5;
        background:white;
        border:3px solid #003DA5;
        &:hover {
          color:#002B79;
          background:rgba(203,216,240,.6);
          border:3px solid #003DA5;
        }
      }
      &.simple-text {
        a {
          color:#003DA5;
        }
      }
    }
  {% elseif content.theme_color.color == 'gold' %}
    .wlt-content-copy {
      .eyebrow {
         border-left:3px #F1BE48 solid;
       }
    }
    .wlt-media {
      {% if content.theme_color.adjust_color == 'lighter' %}
        &:before, &:after {
          background: #FFEAC1;
        }
    {% elseif content.theme_color.adjust_color == 'darker' %}
        &:before, &:after {
          background: #F4CC75;
        }
    {% else %}
      &:before, &:after {
        background:#F1BE48;
      }
    {% endif %}
      .breakdance-image-clip {
        border:9px #F1BE48 solid;
      }
    }
    .wlt-btns {
       .wlt-btn-1 {
        color:#001A51;
        background:#F1BE48;
        &:hover {
          color:#001A51;
          background:#F4CC75;
        }
      }
      .wlt-btn-2 {
        color:#001A51;
        background:white;
        border:3px solid #F1BE48;
        &:hover {
          color:#001A51;
          background:#FEF9EF;
          border:3px solid #F1BE48;
        }
      }
    }
  {% elseif content.theme_color.color == 'teal' %}
    .wlt-content-copy {
      .eyebrow {
        border-left:3px #71DDB7 solid;
      }
    }
    .wlt-media {
      {% if content.theme_color.adjust_color == 'lighter' %}
        &:before, &:after {
          background: #D8F5E9;
        }
    {% elseif content.theme_color.adjust_color == 'darker' %}
        &:before, &:after {
          background: #61C09F;
        }
    {% else %}
      &:before, &:after {
        background:#71DDB7;
      }
    {% endif %}
      .breakdance-image-clip {
        border:#71DDB7 solid 9px;
      }
    }
    .wlt-btns {
     .wlt-btn-1 {
      color:#1D1C1D;
      background:#71DDB7;
      &:hover {
        color:#1D1C1D;
        background:#D8F5E9;
      }
    }
    .wlt-btn-2 {
      color:#1D1C1D;
      background:white;
      border:3px solid #71DDB7;
      &:hover {
        color:#001A51;
        background:#D8F5E9;
        border:3px solid #71DDB7;
      }
    }
  }
  {% endif %}
}
