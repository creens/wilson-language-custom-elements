}
@media only screen and (min-width: 988px) {
  %%SELECTOR%% {
    &.with-media {
      &.with-video {
        &::before, &::after {
          content:'';
          position:absolute;
          top: 0;
          width: 50%;
          height:100%;
        }
        .hero-container {
          flex-direction:row;
        }
      }
    }
  }
}
@media only screen and (min-width: 1080px) {
  %%SELECTOR%% {
    &.with-media {
      &.with-video {
        .hero-container {
          gap:66px;
          .hero-media {
            max-width:600px;
          }
        }
      }
    }
  }
}
@media only screen and (min-width: 1366px) {
  %%SELECTOR%% {
    &.with-media {
      &.with-video {
        .hero-container {
          padding:80px 0;
          .hero-action {
            width:40%;
          }
          .hero-media {
            width:60%;
            max-width:unset;
          }
        }
      }
    }
  }
}