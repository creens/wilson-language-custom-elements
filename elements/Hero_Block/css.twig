%%SELECTOR%% {
  &.with-media {
    &.with-video {
      &::before, &::after {
        content:'';
        position:absolute;
        top: 0;
        width: 100%;
      }
      &:after {
        height:80%;
      }
      .hero-container {
        padding:80px 5%;
        flex-direction:column;
        & > * {
          flex:unset;
        }
      }
      .hero-action {
        width:100%;
      }
      .hero-media {
        margin-top:30px;
        max-width:500px;
        position:relative;
        z-index:10
      }
    }
  }
